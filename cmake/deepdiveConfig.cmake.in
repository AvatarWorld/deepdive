# It defines the following variables
#  DEEPDIVE_INCLUDE_DIRS - include directories for FooBar
#  DEEPDIVE_LIBRARIES    - libraries to link against
 
# Compute paths
get_filename_component(DEEPDIVE_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(DEEPDIVE_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")
 
# Our library dependencies (contains definitions for IMPORTED targets)
if(NOT TARGET deepdive AND NOT deepdive_BINARY_DIR)
  include("${DEEPDIVE_CMAKE_DIR}/deepdiveTargets.cmake")
endif()
 
# These are IMPORTED targets created by deepdiveTargets.cmake
set(DEEPDIVE_LIBRARIES deepdive)